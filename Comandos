Autor=PG
=========================
CISCO_1
=========================
enable
conf ter
hostname RC1
ipv6 unicast-routing
inter g0/0
ipv6 add 2001:a:b:1::1/64
no shut
exit
inter g0/1
ipv6 add 2001:a:b:2::1/64
no shut
exit
inter g0/2
ipv6 add 2001:a:b:3::1/64
no shut
exit
router isis 10
net 49.0010.0000.0000.0001.00
metric-style wide
exit
inter range g0/0-2
ipv6 router isis 10
isis circuit-type level-1
exit
ipv6 dhcp pool LAN
dns-server 2001:a:b:8::255
address prefix 2001:a:b:1::/64
exit
inter g0/0
ipv6 dhcp server LAN
ipv6 nd managed-config-flag 
exit
router isis 10
passive-interface g0/0
exit

